//program to find total & percentage
#include<stdio.h>
main()
{
	int i,marks[6],sum=0,per;
	for(i=0;i<=5;i++)
	{
		printf("enter sub%d marks\t",i+1);
		scanf("%d",&marks[i]);
	}
	printf("the marks are:");
	for(i=0;i<=5;i++)
	{
		sum=sum+marks[i];
	}
	per=float(sum/600.0*100);
	printf("the total marks:%d",sum);
	printf("the percentage is %d",per);
}
